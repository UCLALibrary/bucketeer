###########################################################################
# Default values configuration file for Bucketeer. It's supplied to the
# template file to provide default values. The default values can still be
# overriden by environmental variables using these same variable names.
###########################################################################

## The location of the OpenAPI spec file.
OPENAPI_SPEC_PATH = bucketeer.yaml

## Configures the port on which the microservice listens.
HTTP_PORT = 8888

LAMBDA_S3_BUCKET=lambdabucket
BUCKETEER_S3_BUCKET=srcbucket
BUCKETEER_S3_REGION=us-west-2
BUCKETEER_S3_SECRET_KEY=secretkey
BUCKETEER_S3_ACCESS_KEY=accesskey
BUCKETEER_FS_IMAGE_MOUNT=path
BUCKETEER_FS_IMAGE_PREFIX=UCLAFilePathPrefix
BUCKETEER_FS_CSV_MOUNT=csvpath
BUCKETEER_SLACK_OAUTH_TOKEN=slackoauthtoken
BUCKETEER_SLACK_ERROR_CHANNEL_ID=errorchannel
BUCKETEER_SLACK_CHANNEL_ID=mainchannel
BUCKETEER_IIIF_URL=weburl
BUCKETEER_IIIF_URL_PREFIX=/iiif/2/
HTTP_MAX_QUEUE_SIZE=5
S3_UPLOADER_INSTANCES=1
S3_UPLOADER_THREADS=20
S3_MAX_REQUESTS=20
S3_MAX_RETRIES=30
SOURCE_MAX_FILE_SIZE=300000000
FESTER_URL=http://localhost:9999
LARGE_IMAGE_BUCKETEER=http://localhost:8888
BATCH_CALLBACK_URL=http://localhost:8888/batch/jobs/{}/{}/
BUCKETEER_IIIF_CACHE_USER=username
BUCKETEER_IIIF_CACHE_PASSWORD=password